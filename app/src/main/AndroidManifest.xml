<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xirpla.remedial.bookinghotel">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />

    <application
        android:name="com.xirpla.remedial.bookinghotel.Controller"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.xirpla.remedial.bookinghotel.DetailBelumBayarActivity" />
        <activity android:name="com.xirpla.remedial.bookinghotel.DetailTransaksiActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.BelumBayarActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.MenuTransaksiActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.MenuTransaksiActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.DetailActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity android:name="com.xirpla.remedial.bookinghotel.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity android:name="com.xirpla.remedial.bookinghotel.LoginActivity" />
        <activity android:name="com.xirpla.remedial.bookinghotel.RegisterActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.ProfilActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.PesanActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.PesanActivity1"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.PesanActivity2"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.ListVoucherActivity"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.MenuTransaksiActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.DetailActivity1"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
        <activity
            android:name="com.xirpla.remedial.bookinghotel.DetailActivity2"
            android:parentActivityName="com.xirpla.remedial.bookinghotel.HomeActivity" />
    </application>

</manifest>